{
    "name": "qlimix/validation",
    "type": "library",
    "description": "Validate data individually or by set",
    "license": "MIT",
    "keywords": [
        "validation",
        "validator"
    ],
    "authors": [
        {
            "name": "Frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">7.4"
    },
    "require-dev": {
        "infection/infection": "^0.21",
        "qlimix/code-standard": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\Validation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Qlimix\\Tests\\Validation\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    }
}
